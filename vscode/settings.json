{
    // Editor:
    "editor.accessibilitySupport": "off",
    "editor.codeLens": true,
    "editor.colorDecorators": true,
    "editor.dragAndDrop": false,
    "editor.folding": false,
    "editor.showFoldingControls": "mouseover",
    "editor.inlineSuggest.enabled": true,
    "editor.minimap.enabled": false,
    "editor.fontFamily": "Fira Code",
    "editor.fontLigatures": true,
    "editor.fontSize": 14,
    "explorer.openEditors.visible": 1,
    "editor.renderWhitespace": "selection",
    "editor.renderLineHighlight": "none",
    "editor.hideCursorInOverviewRuler": false,
    "explorer.compactFolders": false,
    "editor.occurrencesHighlight": "singleFile",
    "editor.overviewRulerBorder": false,
    "editor.bracketPairColorization.enabled": false,
    "editor.rulers": [
        120
    ],
    "editor.scrollBeyondLastLine": false,
    "editor.snippetSuggestions": "none",
    "editor.cursorSurroundingLines": 3,
    "editor.useTabStops": true,
    "editor.formatOnPaste": false,
    "editor.wordWrap": "on",
    "editor.wordWrapColumn": 120,
    "editor.hover.enabled": true,
    "editor.stickyScroll.enabled": false,
    "editor.inlayHints.maximumLength": 0,
    "explorer.confirmDelete": false,
    "explorer.decorations.badges": false,
    "explorer.confirmDragAndDrop": false,
    "extensions.ignoreRecommendations": true,
    "diffEditor.renderSideBySide": false,
    "diffEditor.ignoreTrimWhitespace": false,
    "diffEditor.experimental.showMoves": true,
    "breadcrumbs.enabled": false,
    "window.commandCenter": false,
    "window.autoDetectColorScheme": true,
    "workbench.activityBar.location": "hidden",
    "workbench.editor.tabActionCloseVisibility": false,
    "workbench.hover.delay": 3000,
    "editor.scrollbar.vertical": "auto",
    "editor.scrollbar.horizontal": "hidden",
    "workbench.editor.labelFormat": "medium",
    "workbench.startupEditor": "newUntitledFile",
    "workbench.statusBar.visible": true,
    "workbench.tips.enabled": false,
    "workbench.tree.renderIndentGuides": "none",
    "workbench.editor.highlightModifiedTabs": false,
    "workbench.editor.showIcons": false,
    "workbench.editor.showTabs": "single",
    "workbench.editor.decorations.badges": false,
    "workbench.editor.decorations.colors": false,
    "workbench.editor.enablePreview": false,
    "workbench.sideBar.location": "right",
    "workbench.editor.empty.hint": "hidden",
    "workbench.layoutControl.enabled": false,
    "merge-conflict.autoNavigateNextConflict.enabled": false,
    // Use simple navigation:
    "editor.gotoLocation.multipleReferences": "peek",
    "editor.gotoLocation.multipleDefinitions": "peek",
    "editor.gotoLocation.multipleDeclarations": "peek",
    "editor.gotoLocation.multipleImplementations": "peek",
    "editor.gotoLocation.multipleTypeDefinitions": "peek",
    // Code immutable: 
    "editor.autoClosingBrackets": "never",
    "editor.autoClosingQuotes": "never",
    "editor.autoSurround": "never",
    "editor.autoClosingOvertype": "never",
    "editor.suggestOnTriggerCharacters": false,
    "editor.acceptSuggestionOnCommitCharacter": false,
    "editor.acceptSuggestionOnEnter": "on",
    "editor.parameterHints.enabled": false,
    "editor.tabCompletion": "off",
    "editor.suggestSelection": "recentlyUsed",
    "editor.quickSuggestions": {
        "other": "on",
        "comments": "off",
        "strings": "off",
    },
    "editor.formatOnType": false,
    "editor.formatOnSave": true,
    // Files:
    "files.autoGuessEncoding": true,
    "files.associations": {
        "*.csproj": "msbuild"
    },
    "files.autoSave": "onFocusChange",
    "files.exclude": {
        "**/.DS_Store": true,
        "**/node_modules": true,
        "**/bower_components": true,
        "**/__pycache__": true,
        "**/.cache": true,
        "**/.idea": true,
        "**/.pytest_cache": true,
        "**/.ipynb_checkpoints": true,
        "**/.mypy_cache": true,
        "**/.venv": true,
        "**/.coverage": true,
        "**/*.py[co]": true,
        "**/htmlcoverage": true,
        "**/docs/build": true,
        "**/bin": true,
        "**/obj": true,
        "**/log": true,
        "**/logs": true,
        "**/lcov.info": true
    },
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook",
    },
    // Stats and telemetry:
    "dotnetAcquisitionExtension.enableTelemetry": false,
    "continue.telemetryEnabled": false,
    "editor.experimental.treeSitterTelemetry": false,
    "redhat.telemetry.enabled": false,
    "telemetry.telemetryLevel": "off",
    // Terminal:
    "terminal.integrated.shellIntegration.enabled": false,
    "terminal.integrated.shellIntegration.decorationsEnabled": "never",
    "terminal.integrated.stickyScroll.enabled": false,
    "terminal.integrated.enableMultiLinePasteWarning": "never",
    "terminal.integrated.defaultProfile.osx": "zsh",
    "terminal.integrated.defaultProfile.linux": "bash",
    "accessibility.signals.terminalBell": {
        "sound": "on"
    },
    "terminal.integrated.enableVisualBell": true,
    "terminal.integrated.tabs.defaultColor": "terminal.ansiWhite",
    // Git & SCM:
    "git.autofetch": false,
    "git.showInlineOpenFileAction": false,
    "git.autorefresh": true,
    "git.decorations.enabled": true,
    "git.openRepositoryInParentFolders": "never",
    "git.confirmSync": false,
    "scm.graph.pageSize": 25,
    // Tools, extensions, and languages:
    "problems.decorations.enabled": false,
    // Theme:
    "workbench.colorTheme": "Default Light Modern",
    "workbench.iconTheme": "vs-minimal",
    "editor.semanticHighlighting.enabled": "configuredByTheme",
    "editor.unicodeHighlight.nonBasicASCII": false,
    "markdown.extension.syntax.decorationFileSizeLimit": 5000,
    "[xml]": {
        "editor.defaultFormatter": "redhat.vscode-xml"
    },
    "xml.format.maxLineWidth": 120,
    "[msbuild]": {
        "editor.defaultFormatter": "redhat.vscode-xml"
    },
    "yaml.schemas": {
        "file:///Users/atarutin/.vscode/extensions/atlassian.atlascode-3.0.10/resources/schemas/pipelines-schema.json": "bitbucket-pipelines.yml"
    },
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features",
        "editor.folding": true
    },
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "coverage-gutters.coverageFileNames": [
        "lcov.info",
        "coverage.info",
        "cov.xml",
        "coverage.xml",
        "jacoco.xml",
        "coverage.cobertura.xml"
    ],
    "coverage-gutters.showLineCoverage": true,
    "coverage-gutters.showRulerCoverage": true,
    "testing.openTesting": "neverOpen",
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    // Omnisharp:
    "omnisharp.enableEditorConfigSupport": true,
    "omnisharp.enableDecompilationSupport": true,
    "omnisharp.enableLspDriver": true,
    // dotnet: 
    "dotnet.backgroundAnalysis.analyzerDiagnosticsScope": "openFiles",
    "dotnet.backgroundAnalysis.compilerDiagnosticsScope": "openFiles",
    "dotnet.inlayHints.enableInlayHintsForIndexerParameters": true,
    "dotnet.inlayHints.enableInlayHintsForLiteralParameters": true,
    "dotnet.inlayHints.enableInlayHintsForObjectCreationParameters": true,
    "dotnet.inlayHints.enableInlayHintsForOtherParameters": true,
    "dotnet.inlayHints.enableInlayHintsForParameters": true,
    "dotnet.inlayHints.suppressInlayHintsForParametersThatDifferOnlyBySuffix": true,
    "dotnet.inlayHints.suppressInlayHintsForParametersThatMatchArgumentName": true,
    "dotnet.inlayHints.suppressInlayHintsForParametersThatMatchMethodIntent": true,
    // C#:
    "csharp.debug.justMyCode": true,
    "csharp.debug.console": "integratedTerminal",
    "csharp.debug.symbolOptions.searchMicrosoftSymbolServer": true,
    "csharp.debug.symbolOptions.searchNuGetOrgSymbolServer": true,
    "csharp.debug.logging.moduleLoad": false,
    "csharp.suppressBuildAssetsNotification": true,
    "csharp.inlayHints.enableInlayHintsForImplicitVariableTypes": false,
    "csharp.inlayHints.enableInlayHintsForImplicitObjectCreation": false,
    "csharp.inlayHints.enableInlayHintsForLambdaParameterTypes": false,
    "csharp.inlayHints.enableInlayHintsForTypes": false,
    "csharp.format.enable": true,
    "csharp.showOmnisharpLogOnError": true,
    "csharp.suppressHiddenDiagnostics": false,
    "debug.onTaskErrors": "showErrors",
    "search.searchOnType": false,
    "github.copilot.enable": {
        "*": true,
        "plaintext": false,
        "markdown": true,
        "scminput": false
    }
}